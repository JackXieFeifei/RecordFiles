<?xml version="1.0" encoding="UTF-8"?>
<resources>

    <!-- sdk 支付界面 大部分颜色设置标签 -->
    <!-- 顶部标题计费点名称部分字体颜色 对应Demo中“biubiu” -->
    <color name="sdk_title_fee_name_color">#393939</color>
    <!-- 内容区域正常文字部分字体颜色 -->
    <color name="sdk_content_normal_text_color">#666666</color>
    <!-- 按钮文字部分字体颜色 -->
    <color name="sdk_button_text_color">#ffffff</color>
    <!-- sdk顶部标题栏道具价格部分字体颜色 对应Demo中"￥1" -->
    <color name="sdk_title_fee_price_color">#f75c4c</color>
    <!-- 短信支付高亮突出部分字体颜色 -->
    <color name="sdk_sms_content_highlight_text_color">#67bf4b</color>
    <!-- 底部 menu 提示语部分字体颜色 -->
    <color name="sdk_menu_tip_content_text_color">#a3a3a3</color>
    <!-- 底部 menu 标题部分字体颜色 对应Demo中“温馨提示：” -->
    <color name="sdk_menu_tip_head_text_color">#393939</color>
    <!-- 第3方支付字体颜色 -->
    <color name="sdk_other_pay_text_color">#666666</color>
    <!-- 验证码获取按钮不可点击时字体颜色 -->
    <color name="sdk_pay_vertification_normal_text_color">#a3a3a3</color>
    <!-- 验证码可以点击 -->
    <color name="sdk_pay_vertification_highlight_text_color">#67bf4b</color>
    <!-- sdk version name 显示 -->
    <color name="sdk_bottom_version_color">#c8c8c8</color>
    <!-- sdk水平分割线颜色 -->
    <color name="sdk_horizonal_line_divider_color">#c8c8c8</color>
    <!-- 底部 menu 垂直分割线颜色 -->
    <color name="sdk_veritical_line_divider_color">#67bf4b</color>
    
    
    <!-- sdk 支付界面 大部分字体大小设置标签 必须是dp ,and int -->
    <!-- 顶部标题计费点名称部分字体大小 -->
    <dimen name="sdk_title_fee_name_size">20dp</dimen>
    <!-- 顶部标题计费点价格部分字体大小 -->
    <dimen name="sdk_title_fee_price_size">16dp</dimen>
    <!-- 内容区域正常文字部分字体大小 -->
    <dimen name="sdk_content_normal_text_size">16dp</dimen>
    <!-- 按钮文字部分字体大小 -->
    <dimen name="sdk_button_text_size">16dp</dimen>
    <!-- 内容区域高亮突出部分字体大小 -->
    <dimen name="sdk_content_highlight_text_size">14dp</dimen>
    <!-- 验证码提示字体大小 -->
    <dimen name="sdk_pay_vertification_text_size">14dp</dimen>
    <!-- 底部 menu 菜单功能按钮字体大小 -->
    <dimen name="sdk_bottom_highlight_text_size">13dp</dimen>
    <!-- 底部 menu 提示语部分字体大小 -->
    <dimen name="sdk_menu_tip_text_size">12dp</dimen>
    <!-- 短信支付 提示语字体大小 -->
    <dimen name="sdk_sms_content_tip_size">14dp</dimen>
    <!-- 第3方支付字体大小 -->
    <dimen name="sdk_other_pay_text_size">12dp</dimen>
    <!-- version name 字体大小控制 -->
    <dimen name="sdk_bottom_version_size">10dp</dimen>

</resources>